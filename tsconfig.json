{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "ESNEXT",
      "module": "commonjs",
      "moduleResolution": "node",
      "declaration": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictFunctionTypes": true,
      "alwaysStrict": true
   }
}